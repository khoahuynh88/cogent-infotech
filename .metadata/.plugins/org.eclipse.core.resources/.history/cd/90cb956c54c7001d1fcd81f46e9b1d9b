package cogent_batch65;

public class HelloWorld {
	public static void main (String[] args){
		System.out.println("Welcom to cogent infotech!!");
		
		byte b =100;
		int i =b;
		
		System.out.println(i);
		
		long l =i;
		System.out.println("l is =" +l);
		
		char ch='A';
		int x =ch;
		System.out.println("x is =" +x);
	}
}
